{
    "name": "private-gpt-ui",
    "version": "0.0.1",
    "description": "Web Application for GPT",
    "main": "index.js",
    "scripts": {
        "webpack:prod": "cross-env NODE_ENV=production webpack --config webpack/prod.js",
        "webpack:qa": "cross-env NODE_ENV=qa webpack --config webpack/qa.js",
        "webpack:dev": "cross-env NODE_ENV=dev webpack-dev-server --config webpack/dev.js",
        "format": "prettier --write src/**/**/*.{js,ts,tsx} src/*.{js,ts,tsx} src/**/*.{js,ts,tsx}",
        "lint": "eslint \"src/**/**/*.{js,ts,tsx}\"",
        "clean": "rm -rf build/**",
        "safety-check": "npm run lint && npm run tsc && node scripts/coverageStaged.js",
        "safety-check-without-coverage": "npm run lint && npm run tsc",
        "safety-check-post-commit": "npm run lint && npm run tsc && node scripts/coverageBranch.js",
        "test": "jest",
        "cirdep": "madge src/**/**/*.* -c",
        "tsc": "tsc",
        "start": "npm run webpack:dev",
        "build:prod": "npm run clean && npm run lint && npm run webpack:prod",
        "build:qa": "npm run clean &&npm run safety-check-without-coverage && npm run webpack:qa",
        "bundle-report": "webpack-bundle-analyzer --port 4200 compilation-stats.json"
    },
    "targets": "> 0.25%, not dead",
    "author": "vineet.kumar@privado.in",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.21.0",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.21.0",
        "@commitlint/cli": "^17.4.4",
        "@commitlint/config-conventional": "^17.4.4",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
        "@svgr/webpack": "^6.5.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@types/axios": "^0.14.0",
        "@types/jest": "^29.4.0",
        "@types/node": "^18.14.0",
        "@types/ramda": "^0.28.23",
        "@types/react": "^18.0.28",
        "@types/react-custom-scrollbars": "^4.0.10",
        "@types/react-datepicker": "^4.8.0",
        "@types/react-dom": "^18.0.11",
        "@types/react-router-dom": "^5.1.5",
        "@types/styled-components": "^5.1.26",
        "@types/webpack": "^5.28.0",
        "@types/webpack-dev-server": "^4.7.2",
        "@typescript-eslint/eslint-plugin": "^5.53.0",
        "@typescript-eslint/parser": "^5.53.0",
        "babel-jest": "^29.4.3",
        "babel-loader": "^9.1.2",
        "babel-plugin-macros": "^3.1.0",
        "babel-plugin-styled-components": "2.0.7",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "circular-dependency-plugin": "^5.2.2",
        "copy-webpack-plugin": "^11.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.3",
        "dotenv-webpack": "^8.0.1",
        "eslint": "^8.34.0",
        "eslint-config-airbnb-typescript": "17.0.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^7.3.0",
        "html-loader": "^4.2.0",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^8.0.3",
        "jest": "^29.4.3",
        "madge": "^6.0.0",
        "minimist": "^1.2.8",
        "prettier": "2.8.4",
        "progress-bar-webpack-plugin": "^2.1.0",
        "react-refresh": "^0.14.0",
        "speed-measure-webpack-plugin": "^1.5.0",
        "style-loader": "^3.3.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.2",
        "typescript": "^4.9.5",
        "url-loader": "^4.1.1",
        "webpack": "^5.75.0",
        "webpack-bundle-analyzer": "^4.8.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.11.1",
        "webpack-merge": "^5.8.0"
    },
    "engines": {
        "node": ">=14"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@types/react-pdf": "^6.2.0",
        "@types/react-syntax-highlighter": "^15.5.6",
        "axios": "^1.3.3",
        "html-to-image": "^1.11.11",
        "html2canvas": "^1.4.1",
        "moment": "^2.29.4",
        "oidc-client-ts": "^2.2.4",
        "qrcode.react": "^3.1.0",
        "ramda": "^0.28.0",
        "react": "^18.2.0",
        "react-checkbox-tree": "^1.8.0",
        "react-custom-scrollbars": "^4.2.1",
        "react-d3-tree": "^3.5.0",
        "react-datepicker": "^4.10.0",
        "react-dom": "^18.2.0",
        "react-otp-field": "^3.0.1",
        "react-pdf": "^6.2.2",
        "react-query": "^3.39.3",
        "react-router-dom": "^5.2.0",
        "react-syntax-highlighter": "^15.5.0",
        "react-to-print": "^2.14.12",
        "react-tooltip": "^5.11.2",
        "reactflow": "^11.5.1",
        "regenerator-runtime": "^0.13.11",
        "styled-components": "^5.3.6",
        "xlsx": "^0.18.5"
    },
    "jest": {
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 90,
                "lines": 90,
                "statements": 90
            }
        },
        "roots": [
            "src"
        ],
        "verbose": true,
        "moduleNameMapper": {
            "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/svgrMock.tsx",
            "^.+\\.(css|less)$": "<rootDir>/src/__mocks__/styledMock.tsx"
        },
        "testMatch": [
            "**/__tests__/**/*.+(ts|tsx)",
            "**/?(*.)+(spec|test).+(ts|tsx)"
        ],
        "setupFilesAfterEnv": [
            "./src/test/setupFilesAfterEnv.ts"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "^.+\\.(js|ts|tsx)$": "babel-jest"
        },
        "testTimeout": 30000,
        "setupFiles": [
            "./src/test/setup.ts"
        ]
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "npm run safety-check",
            "pre-push": "npm run safety-check-post-commit"
        }
    },
    "resolutions": {
        "@types/react": "^18.0.0"
    }
}
